def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "K"
  select_item
  scroll_up

def_key "J"
  select_item
  scroll_down

def_key "h"
  previous_column

def_key "l"
  next_column

def_key "L"
  show_lyrics

def_key "+"
  volume_up

def_key "-"
  jump_to_parent_directory

def_key "G"
  move_end

def_key "g"
  move_home
