snippet sieve "Thanks jls"
minp, primes = [], []

def sieve(n):
    minp = [0 * x for x in range(n+1)]
    
    for i in range(2, n+1):
        if minp[i] == 0:
            minp[i] = i
            primes.append(i)

        for p in primes:
            if (i * p) > n:
                break
            minp[i * p] = p
            if p == minp[i]:
                break

endsnippet
